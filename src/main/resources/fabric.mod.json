{
	"schemaVersion": 1,
	"id": "joyeux-lib",
	"version": "${version}",
	"name": "Joyeux Lib",
	"description": "A Libary to help make coding Joyeux mods easier!",
	"authors": [
		"Joyeux"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "GNU Lesser General Public License v3",
	"icon": "assets/joyeux-lib/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.joyeuxlib.JoyeuxLib"
		],
		"client": [
			"com.joyeuxlib.JoyeuxLibClient"
		],
		"fabric-datagen": [
			"com.joyeuxlib.JoyeuxLibDataGenerator"
		]
	},
	"mixins": [
		"joyeux-lib.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"custom": {
		"modmenu": {
			"badges": [ "library" ]
		}
	}
}